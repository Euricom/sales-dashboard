generator client {
    provider = "prisma-client-js"
}

datasource db {
    provider = "mongodb"
    url      = env("DATABASE_URL")
}

model Employee {
    id   String   @id @default(uuid()) @map("_id")
    rows String[] @default(["0"])
}

